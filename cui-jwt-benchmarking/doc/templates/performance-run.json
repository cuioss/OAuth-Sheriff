{
  "timestamp": "${TIMESTAMP}",
  "commit": "${COMMIT_HASH}",
  "performance": {
    "score": ${PERFORMANCE_SCORE},
    "throughput": {
      "value": ${THROUGHPUT_VALUE},
      "unit": "ops/s"
    },
    "averageTime": {
      "value": ${AVERAGE_TIME_SEC},
      "unit": "s"
    },
    "errorResilience": {
      "value": ${ERROR_RESILIENCE_VALUE},
      "unit": "ops/s"
    }
  },
  "rawMetrics": {
    "throughputOpsPerSec": ${THROUGHPUT_OPS_PER_SEC},
    "avgTimeInMicros": ${AVG_TIME_MICROS},
    "errorResilienceOpsPerSec": ${ERROR_RESILIENCE_OPS_PER_SEC}
  },
  "environment": {
    "javaVersion": "${JAVA_VERSION}",
    "jvmArgs": "${JVM_ARGS}",
    "osName": "${OS_NAME}"
  }
}